{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "google",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "linebreak-style": [
            "error",
            "windows"
        ],
        "object-curly-spacing": [
            "off"
        ],
        "comma-dangle": ["error", {
            "objects": "always",
            "arrays": "always"
        }],
        "max-len": "off"
    }
}